
{
  "version": 2,
  "routes": [
    {
      "src": "/api/scraper",
      "dest": "/api/scraper.py"
    },
    {
      "src": "/api/verifier",
      "dest": "/api/verifier.go"
    }
  ],
  "functions": {
    "api/scraper.py": {
      "maxDuration": 60
    },
    "api/verifier.go": {
      "maxDuration": 60
    }
  },
  "build": {
    "env": {
      "GO_BUILD_FLAGS": "-ldflags '-s -w'"
    }
  }
}